<XMLlist>
    <elementSet setName="Dublin Core" recordType="All">
        <element>Title</element>
        <element>Description</element>
        <element>Creator</element>
        <element>Date</element>
        <element>Rights</element>
		<!-- This element is merged into Notes. -->
<!--        <element>Format</element>-->
        <element>Identifier</element>
        <element>Episode Title</element>
        <element>Series Title</element>
        <element>Music/Sound Used</element>
        <element>Notes</element>
        <element>Physical Format</element>
        <element>Physical Location</element>
<!--         <element>Digital Location</element> -->
<!--         <element>Transcription</element> -->
    </elementSet>

    <elementSet setName="Item Type Metadata" recordType="Item">
        <element>Duration</element>
        <element>Interviewer</element>
        <element>Interviewee</element>
        <element>Broadcast Date</element>
        <element>Host</element>
    </elementSet>
</XMLlist>
