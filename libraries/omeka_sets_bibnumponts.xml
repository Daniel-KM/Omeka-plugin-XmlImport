<XMLlist>
    <elementSet setName="Dublin Core" recordType="All">
        <element>Identifier</element>
        <element>Type</element>
        <element>Title</element>
        <element>Creator</element>
        <element>Description</element>
        <element>Publisher</element>
        <element>Date</element>
        <element>Rights</element>
        <element>Format</element>
        <element>Source</element>
        <element>Relation</element>
    </elementSet>

    <!-- Éléments actuellement non récupérés. -->
    <elementSet setName="None" recordType="Item">
        <element>Date Created</element>
        <!-- Compte tenu des spécifications de numérisation, le nombre d'objet est toujours égal au nombre d'images. -->
        <element>Nombre vue objet</element>
        <element>Identifiant support</element>
        <element>Objet associé</element>
        <element>Objet associé date</element>
        <element>Commentaire type</element>
        <element>Commentaire date</element>
        <element>Commentaire</element>
    </elementSet>
</XMLlist>
